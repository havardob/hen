<footer class="footer">
    <div class="constraint">
        <div class="callout">
            <h2 class="callout-title">{{ siteFooter.title }}</h2>
            {% if siteFooter.footerLinks %}  
                <div class="callout-actions">
                    {% for item in siteFooter.footerLinks %}
                        <a href="{% if item.linkType == "isInternal" %}/{{ item.internalUrl.slug.current }}{% endif %}{% if item.linkType == "isExternal" %}{{ item.externalUrl }}{% endif %}" class="cta{{ " | primary" if item.isPrimary }}">
                            <span>{{ item.linkText }}</span>
                            {% if item.linkIcon %}
                                {% include "icons/" + item.linkIcon + ".svg" ignore missing %}
                            {% endif %}
                        </a>
                    {% endfor %}
                </div>
            {% endif %}
        </div>
    </div>
